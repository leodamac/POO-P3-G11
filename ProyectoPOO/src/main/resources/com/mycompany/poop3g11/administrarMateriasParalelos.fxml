<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="265.0" prefWidth="300.0" spacing="20.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="configuracionControllers.AdministrarMateriasParalelosController">
    <children>
        <Label text="Administrar Materias y Paralelos" styleClass="titulo">
         <font>
            <Font name="System Bold" size="15.0" />
         </font></Label>
        <Button minHeight="30.0" minWidth="400.0" onAction="#switchToIngresarMateria" prefHeight="30.0" prefWidth="400.0" text="Ingresar Materia">
         <font>
            <Font size="14.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
        <Button minHeight="30.0" minWidth="400.0" mnemonicParsing="false" onAction="#switchToEditarMateria" prefHeight="30.0" prefWidth="400.0" text="Editar Materia">
         <font>
            <Font size="14.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
        <Button minHeight="30.0" minWidth="400.0" mnemonicParsing="false" onAction="#switchToAgregarParalelo" prefHeight="30.0" prefWidth="400.0" text="Agregar Paralelo">
         <font>
            <Font size="14.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button minHeight="30.0" minWidth="400.0" mnemonicParsing="false" onAction="#switchToEliminarParalelo" prefHeight="30.0" prefWidth="400.0" text="Eliminar Paralelo">
         <font>
            <Font size="14.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
        <Button minHeight="30.0" minWidth="400.0" mnemonicParsing="false" onAction="#switchToConfiguracion" prefHeight="30.0" prefWidth="400.0" text="Salir">
         <font>
            <Font size="14.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
