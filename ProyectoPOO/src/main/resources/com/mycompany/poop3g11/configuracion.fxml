<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="contenedorVBox" alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.poop3g11.ConfiguracionController">
    <children>
        <Label text="CONFIGURACIÓN" styleClass="titulo">
         <font>
            <Font name="System Bold" size="15.0" />
         </font></Label>
        <Button fx:id="administrarTerminosAcademicosButton" minHeight="30.0" minWidth="400.0" onAction="#switchToAdministrarTerminosAcademicos" prefHeight="30.0" prefWidth="400.0" text="Administrar términos académicos">
         <font>
            <Font size="14.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
        <Button fx:id="administrarMateriasParalelosButton" minHeight="30.0" minWidth="400.0" mnemonicParsing="false" onAction="#switchToAdministrarMateriasParalelos" prefHeight="30.0" prefWidth="400.0" text="Administrar materias y paralelos">
         <font>
            <Font size="14.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
        <Button fx:id="administrarPreguntasButton" minHeight="30.0" minWidth="400.0" mnemonicParsing="false" onAction="#switchToAdministrarPreguntas" prefHeight="30.0" prefWidth="400.0" text="Administrar preguntas">
         <font>
            <Font size="14.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
        <Button fx:id="salirButton" minHeight="30.0" minWidth="400.0" mnemonicParsing="false" onAction="#switchToMenuPrincipal" prefHeight="30.0" prefWidth="400.0" text="Salir">
         <font>
            <Font size="14.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
    </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
